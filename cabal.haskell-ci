branches:       master

doctest:        <9.11
doctest-skip:   effectful-plugin

tests:          True
benchmarks:     True

jobs-selection: any

installed:      -all

raw-project
  package effectful-core
    ghc-options: -Werror

  package effectful-plugin
    ghc-options: -Werror

  package effectful-th
    ghc-options: -Werror

  package effectful
    ghc-options: -Werror
    flags: +benchmark-foreign-libraries
